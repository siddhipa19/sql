CREATE DATABASE GOA;

USE GOA;

CREATE TABLE venues (
  name TEXT,
  phone TEXT,
  latitude DOUBLE,
  longitude DOUBLE,
  address TEXT,
  landmark TEXT,
  city TEXT,
  city_oid TEXT
);

